<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:usch="http://www.unisoftware.ru/schematron-extensions" xmlns:sql="urn:schemas-microsoft-com:mapping-schema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Файл">
		<xs:annotation>
			<!-- -******************************************************************************************************************************************************************************* -->
			<xs:appinfo>
				<sch:pattern name="Проверка имени файла на соответствие значению атрибута @ИдФайл">
					<sch:rule context="Файл">
						<!--   Проверка имени файла на соответствие ИдФайл                                                                              -->
						<sch:assert test="usch:getFileName() = @ИдФайл">
							<usch:error code="0400400007" path="@ИдФайл" value-of-select="@ИдФайл" doc-id-path="usch:getFileName()" doc-id-caption="Файл">
							Имя файла обмена "
							<usch:value-of select="usch:getFileName()"/>
							" не совпадает со значением элемента ИдФайл "
							<usch:value-of select="@ИдФайл"/>
							"
							</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!-- -******************************************************************************************************************************************************************************* -->
			<xs:documentation>Файл обмена</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ИдОтпр" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сведения об отправителе</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ФИООтв" type="ФИОТип">
								<xs:annotation>
									<xs:documentation>Фамилия, имя, отчество ответственного лица</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="ДолжОтв" use="optional">
							<xs:annotation>
								<xs:documentation>Должность ответственного лица</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="45"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Тлф" use="optional">
							<xs:annotation>
								<xs:documentation>Номер контактного телефона</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="E-mail" use="optional">
							<xs:annotation>
								<xs:documentation>E-mail</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:maxLength value="45"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Документ" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Состав и структура документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвРАФП">
								<xs:annotation>
									<xs:documentation>Общедоступные сведения из государственного реестра аккредитованных филиалов, представительств иностранных юридических лиц</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="СвИЮЛАкт">
											<xs:annotation>
												<xs:documentation>Сведения об иностранном юридическом лице</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="НаимИЮЛ">
														<xs:annotation>
															<xs:documentation>Наименование иностранного юридического лица</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="НаимИЮЛПолн" use="required">
																<xs:annotation>
																	<xs:documentation>Полное наименование иностранного юридического лица</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="1000"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="НаимИЮЛЛат" use="optional">
																<xs:annotation>
																	<xs:documentation>Полное наименование иностранного юридического лица (в латинской транскрипции)</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="1000"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="НаимИЮЛСокр" use="optional">
																<xs:annotation>
																	<xs:documentation>Сокращенное наименование иностранного юридического лица</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="АдрИЮЛИнк">
														<xs:annotation>
															<xs:documentation>Адрес в стране регистрации (инкорпорации)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="СтрРег" type="ОКСМТип" use="required">
																<xs:annotation>
																	<xs:documentation>Код страны регистрации (инкорпорации)</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="НаимСтр" use="required">
																<xs:annotation>
																	<xs:documentation>Наименование страны регистрации (инкорпорации)</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="АдрИЮЛРег" use="required">
																<xs:annotation>
																	<xs:documentation>Адрес в стране регистрации (инкорпорации)</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="РегИЮЛИнк">
														<xs:annotation>
															<xs:documentation>Сведения о регистрации в стране регистрации (инкорпорации)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="НаимРегОрг" use="required">
																<xs:annotation>
																	<xs:documentation>Наименование регистрирующего органа</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="РегНомер" use="required">
																<xs:annotation>
																	<xs:documentation>Регистрационный номер</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="70"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ОсобРежРег" use="optional">
																<xs:annotation>
																	<xs:documentation>Особенности режима регистрации</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="КодИЮЛИнк" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сведения о коде налогоплательщика в стране регистрации (инкорпорации)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="КодНП" use="required">
																<xs:annotation>
																	<xs:documentation>Код налогоплательщика или его аналог</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="50"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="УставИЮЛ">
														<xs:annotation>
															<xs:documentation>Сведения об уставном капитале</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="УстКап" use="required">
																<xs:annotation>
																	<xs:documentation>Размер уставного капитала</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:integer">
																		<xs:totalDigits value="20"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="КодВалют" type="ОКВТип" use="required">
																<xs:annotation>
																	<xs:documentation>Код валюты</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="СвАФПАкт">
											<xs:annotation>
												<xs:documentation>Сведения о филиале, представительстве иностранного юридического лица на территории Российской Федерации</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="НаимАФП">
														<xs:annotation>
															<xs:documentation>Наименование филиала, представительства иностранного юридического лица</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="НаимАФППолн" use="required">
																<xs:annotation>
																	<xs:documentation>Полное наименование филиала, представительства иностранного юридического лица</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="1000"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="НаимАФПСокр" use="optional">
																<xs:annotation>
																	<xs:documentation>Сокращенное наименование филиала, представительства иностранного юридического лица</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="1000"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="АдрАФПРФ">
														<xs:annotation>
															<xs:documentation>Адрес (место нахождение) на территории Российской Федерации)</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:choice>
																<xs:element name="АдрФИАС" type="АдрФИАСТип">
																	<xs:annotation>
																		<xs:documentation>Адрес по ФИАС</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="АдрКЛАДР" type="АдрКЛАДРТип">
																	<xs:annotation>
																		<xs:documentation>Адрес по КЛАДР</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:choice>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="СвАк">
														<xs:annotation>
															<xs:documentation>Сведения об аккредитации филиала, представительства</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="НЗА" use="required">
																<xs:annotation>
																	<xs:documentation>Номер записи об аккредитации (НЗА)</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:length value="11"/>
																		<xs:pattern value="[0-9]{11}"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаАк" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата аккредитации (внесения сведений об аккредитации в РАФП)</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="КодНОАк" type="СОНОТип" use="required">
																<xs:annotation>
																	<xs:documentation>Код налогового органа, уполномоченного на аккредитацию филиалов, представительств иностранных юридических лиц</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="НаимНО" use="required">
																<xs:annotation>
																	<xs:documentation>Налоговый орган, уполномоченный на аккредитацию филиалов, представительств иностранных юридических лиц</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="АдрНО" use="required">
																<xs:annotation>
																	<xs:documentation>Адрес налогового органа, уполномоченного на аккредитацию филиалов, представительств иностранных юридических лиц</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="1000"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="СвСостАк" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сведения о состоянии аккредитации филиала, представительства иностранного юридического лица</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="СостАк" use="required">
																<xs:annotation>
																	<xs:documentation>Состояние аккредитации</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="СвПрекрАк" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сведения о прекращении аккредитации филиала, представительства</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="ДатаПрекрАк" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата прекращения действия аккредитации (внесения сведений о прекращении действия аккредитации в РАФП)</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="СвУчетНО" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Сведения об учете в налоговом органе</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="required">
																<xs:annotation>
																	<xs:documentation>ИНН</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="КПП" type="КППТип" use="required">
																<xs:annotation>
																	<xs:documentation>КПП</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="ДатаПостУч" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата постановки на учет</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="КодНОУч" type="СОНОТип" use="required">
																<xs:annotation>
																	<xs:documentation>Код налогового органа, осуществившего постановку на учет</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="НаимНОУч" use="required">
																<xs:annotation>
																	<xs:documentation>Наименование налогового органа, осуществившего постановку на учет</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>
																		<xs:maxLength value="255"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ДатаВнесРАФП" type="ДатаТип" use="required">
																<xs:annotation>
																	<xs:documentation>Дата внесения в РАФП записи, содержащей указанные сведения</xs:documentation>
																</xs:annotation>
